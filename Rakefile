require_relative './fetchers/towns'
require_relative './fetchers/teams'
require_relative './fetchers/players'

namespace :towns do
  task :fetch_all do
    fetch_all_towns
  end
end

namespace :teams do
  task :fetch_all do
    fetch_all_teams
  end
end

namespace :players do
  task :fetch_all do
    fetch_all_players
  end
end
